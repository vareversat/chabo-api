{
	"name": "Go",
	"dockerComposeFile": "../compose.yml",
	"service": "app",
	"workspaceFolder": "/app",	
	"customizations": {
		"vscode": {
			"settings": {},
			"extensions": [
				"k--kato.intellij-idea-keybindings",
				"golang.go",
				"ms-azuretools.vscode-docker",
				"ms-vscode.makefile-tools"
			]
		}
	},
	"portsAttributes": {
		"8080": {
			"label": "API Port",
			"onAutoForward": "notify"
		}
	},
	"overrideCommand": false
}